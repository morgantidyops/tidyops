<?xml version="1.0" encoding="UTF-8"?>
<ValidationRule xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>CHANNEL_ORDERS__Floor_price_is_required</fullName>
    <active>true</active>
    <errorConditionFormula>AND( ISPICKVAL(CHANNEL_ORDERS__Pricing_Type__c ,&quot;PNR&quot;),ISNULL( CHANNEL_ORDERS__Floor_Price__c ))</errorConditionFormula>
    <errorDisplayField>CHANNEL_ORDERS__Floor_Price__c</errorDisplayField>
    <errorMessage>Floor Price is required if Pricing Type = PNR</errorMessage>
</ValidationRule>
